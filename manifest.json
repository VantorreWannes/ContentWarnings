{
    "manifest_version": 2,
    "name": "Content Warnings extention",
    "version": "1.0",
    "description": "Warns about user-defined keywords on webpages",
    "permissions": ["activeTab", "storage"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["print_all.js"]
      }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    }
  }
  